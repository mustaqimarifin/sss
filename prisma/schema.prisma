datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model guestbook {
  id         Int       @id @default(autoincrement())
  email      String
  name       String
  body       String
  image      String?
  created_by String
  created_at DateTime  @default(now())
  updated_at DateTime? @updatedAt
}

model views {
  slug  String @id @db.VarChar(128)
  count Int    @default(1)
}
